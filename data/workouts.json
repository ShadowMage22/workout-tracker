          "type": "cooldown",
          "title": "Cooldown (5 minutes)",
          "items": [
            {
              "name": "Chest Stretch",
              "instructions": {
                "sets": "",
                "rest": "",
                "time": "30s/side",
                "targets": "chest",
                "notes": "Setup: Stand in a doorway or by a wall with one forearm on the frame and the chest lifted. Move: Step forward until you feel a gentle stretch and hold it for 20\u201330 seconds, easing back a step if the stretch is too intense. Breathe: Take slow 4-count inhales and 6-count exhales to let the chest soften. Avoid: Avoid shrugging or forcing range, and keep the elbow lower if the shoulder feels pinchy.",
                "durationSeconds": 30
              },
              "id": "push-cooldown-chest-stretch",
              "itemType": "cooldown"
            },
            {
              "name": "Shoulder Stretch",
              "instructions": {
                "sets": "",
                "rest": "",
                "time": "30s/side",
                "targets": "shoulders",
                "notes": "Setup: Stand tall with one arm across your chest and the opposite hand supporting just above the elbow. Move: Gently pull the arm in until you feel a mild stretch and hold for 20\u201330 seconds, keeping the torso square and lowering the arm if needed. Breathe: Breathe slowly and evenly, letting each exhale relax the shoulder. Avoid: Avoid twisting the torso or hiking the shoulder toward the ear.",
                "durationSeconds": 30
              },
              "id": "push-cooldown-shoulder-stretch",
              "itemType": "cooldown"
            },
            {
              "name": "Breathing",
              "instructions": {
                "sets": "",
                "rest": "",
                "time": "1\u20132 min",
                "targets": "recovery",
                "notes": "Setup: Lie on your back with knees bent or sit tall with relaxed ribs and a long neck. Move: Expand the belly and side ribs on the inhale, pause for a second, then let them soften on the exhale, shortening the range if you feel lightheaded. Breathe: Use a 4-count inhale and a 6-count exhale to downshift. Avoid: Avoid shallow chest-only breaths or breath holding.",
                "durationSecondsMin": 60,
                "durationSecondsMax": 120
              },
              "id": "push-cooldown-breathing",
              "itemType": "cooldown"
            }
          ]
        }
